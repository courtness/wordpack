{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "indent": [
      "error", 2
    ],
    "no-multiple-empty-lines": [
      "error",
      "never"
    ],
    "quotes": [
      2,
      "backtick"
    ],
    "semi": [
      2,
      "always"
    ]
  }
}